---
import fetchData from "../lib/fetchData";
import type FetchDataProps from "../interfaces/FetchData";
import Layout from "../layouts/Layout.astro";
import Hero from "../components/sections/index/Hero.tsx";
import Infographic01 from "../components/sections/index/Infographic01.tsx";
import Infographic02 from "../components/sections/index/Infographic02.tsx";
import Services from "../components/sections/index/Services.tsx";
import Reviews from "../components/sections/index/Reviews.tsx";
import Cta01 from "../components/sections/index/Cta01.tsx";
import Faq from "../components/sections/index/Faq.tsx";
import type { FaqItemProps } from "../components/FaqItem.tsx";

const query = `
query Query {
	faqModel {
		question
		answer
	}
}
`;

const { faqModel: data }: FetchDataProps = await fetchData(query);
---

<Layout title="Human Design" description="">	
  <Hero client:load />
  <Infographic01 client:load />
  <Infographic02 client:load />
  <Services client:load />
  <Reviews client:load />
  <!-- <Cta01 /> -->
  <!-- <Faq faqItems={data as FaqItemProps[]} client:load /> -->
</Layout>